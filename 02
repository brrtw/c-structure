#include <stdio.h>
#include <math.h>

typedef struct{
    double x, y;
}p;

int main(){
   p p1;
   p p2;
   printf("infome as x e y: ");
   scanf("%lf %lf ", &p1.x, &p1.y);
   printf("infome as x e y: ");
   scanf("%lf %lf ", &p2.x, &p2.y);
   double dist;
   dist= sqrt(pow(p2.x - p1.x, 2) + pow(p2.y - p1.y, 2));
   printf("A distancia dos pontos e: %lf", dist);
   
    return 0;
}
